<h1><%=  @album.name %></h1>

<section>
  <header><b>Tracks</b></header>

  <ul>
     <%= render @tracks %>
  </ul>
</section>

<nav>
  <header><b>Links</b></header>

  <ul>
    <li><%= link_to "New Track", new_album_track_url(@album) %></li>
    <li><%= link_to "Edit Album", edit_album_url(@album), method: :get %></li>
    <li><%= link_to "Delete Album", album_url(@album), method: :delete %></li>
    <li><%= link_to "Back to #{@band.name}", band_url(@band) %></li>
  </ul>
</nav>
